{
  "name": "@thedeveloper/qwik-supabase",
  "type": "module",
  "version": "0.2.1",
  "description": "A simple wrapper around Supabase.js to enable usage within Qwik.",
  "homepage": "https://github.com/thedeveloper/qwik-supabase#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build.lib && npm run build.types",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint --ext=js,ts,jsx,tsx .",
    "lint:fix": "eslint --ext=js,ts,jsx,tsx . --fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.6"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.11.1",
    "@types/eslint": "^8.4.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "bumpp": "^8.2.1",
    "esbuild": "^0.14.53",
    "eslint": "^8.21.0",
    "eslint-plugin-qwik": "^0.11.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  },
  "keywords": [
    "supabase",
    "qwik"
  ],
  "contributors": [
    {
      "name": "tmns",
      "email": "tmn5@pm.me"
    },
    {
      "name": "thedeveloper",
      "email": "mail@thdvlpr.com"
    }
  ],
  "license": "MIT"
}
